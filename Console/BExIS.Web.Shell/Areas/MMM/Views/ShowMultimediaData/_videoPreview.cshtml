@model string
@{ 
    string path = @Url.Content("~/mmm/ShowMultimediaData/getFile/?path=" + Server.UrlEncode(Model));
    string stream = @Url.Content("~/mmm/ShowMultimediaData/getFileStreamResult/?path=" + Server.UrlEncode(Model));
}
<video controls>
    <source src="@stream">
    <source src="@path">
    Your browser does not support the video tag.
</video>
