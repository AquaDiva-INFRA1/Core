@using Telerik.Web.Mvc.UI;
@using BExIS.Web.Shell.Areas.RBM.Models.Booking;

@model List<PersonInConstraint>

 <ul id="userList">
@for (int i = 0; i < Model.Count(); i++)
{
<li id="@Model[i].UserId" class="fullName">@Model[i].UserFullName <input type="hidden" name="user" value="@Model[i].UserId"  />
    <span id="removeUser" class="bx bx-trash" onclick="onConfirmDeleteUser(@Model[i].Index, @Model[i].UserId, 'window')"></span></li>
}
</ul>





