@using Telerik.Web.Mvc.UI
@using BExIS.Web.Shell.Areas.RBM.Models.Booking;

@model List<ActivityEventModel>


<ul id="activityList">
    @for (int i = 0; i < Model.Count(); i++)
    {
        <li id="@Model[i].Id" class="fullName" style="font-size:10px;">
            @Model[i].Name <input type="hidden" name="user" value="@Model[i].Id" />
        @if (Model[i].EditMode && Model[i].EditAccess)
        {
            <span id="removeActivity" class="bx bx-trash" onclick="RemoveActivityFromSchedule(@Model[i].Id, @Model[i].Index)"></span>
        }
        
    </li>
    }
</ul>

