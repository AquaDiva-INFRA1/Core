@model Dictionary<string, List<BExIS.Modules.Dcm.UI.Models.OntologyMappingSuggestionModel>>
@foreach(string category in Model.Keys)
{
    //For each category, create a separate dropdown
    <div class="suggestionContainer">
        <label for="@category annotation">@category</label>
        <select name="@category annotation">
            @foreach(BExIS.Modules.Dcm.UI.Models.OntologyMappingSuggestionModel suggestion in Model[category])
            {
                //For each suggestion create an option
                <option title="@suggestion.conceptURI">@suggestion.displayName</option>
            }
        </select>
    </div>
}

<style>
    .suggestionContainer{
        display: block;
    }
    .suggestionContainer label {
        float: left;
        clear: left;
    }
    .suggestionContainer select{
        float: right;
    }
</style>