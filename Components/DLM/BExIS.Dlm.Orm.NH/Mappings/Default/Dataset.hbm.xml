    <property name="ExtendedPropertyValues" type="System.Xml.XmlDocument, System.Xml, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <column name="ExtendedPropertyValues" not-null="false" sql-type="xml" />
