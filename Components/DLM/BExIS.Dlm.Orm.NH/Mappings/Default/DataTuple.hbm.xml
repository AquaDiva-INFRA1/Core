<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="BExIS.Dlm.Entities" namespace="BExIS.Dlm.Entities.Data">
  <class xmlns="urn:nhibernate-mapping-2.2" name="DataTuple" table="DataTuples">
    <id name="Id" type="Int64">
      <column name="Id" />
      <generator class="native" />
    </id>
    <version name="Version" type="Int32">
      <column name="Version" />
    </version>
    <property name="OrderNo" type="Int32">
      <column name="OrderNo" />
    </property>
    <property name="Content" type="System.Xml.XmlDocument, System.Xml, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <column name="Content" not-null="false" sql-type="xml" />
    </property>
    <many-to-one class="Dataset" name="Dataset" column="DatasetRef"/>          
  </class>

</hibernate-mapping>