<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="BExIS.Security.Entities" namespace="BExIS.Security.Entities.Authentication">
  <class xmlns="urn:nhibernate-mapping-2.2" name="Authenticator" table="Authenticators" dynamic-update="true">

    <id name="Id" type="Int64">
      <column name="Id" index="idx_Authenticators_id"/>
      <generator class="native" />
    </id>

    <version name="VersionNo" type="Int32">
      <column name="VersionNo" />
    </version>

    <property name="Extra" type="System.Xml.XmlDocument, System.Xml, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <column name="Extra" not-null="false" sql-type="xml" />
    </property>

    <property name="Name" type="String" >
      <column name="Name" index="idx_Authenticators_name"/>
    </property>
    <property name="AssemblyPath" type="String" >
      <column name="AssemblyPath" />
    </property>
    <property name="ClassPath" type="String" >
      <column name="ClassPath" />
    </property>
    <property name="ConnectionString" type="String" >
      <column name="ConnectionString" />
    </property>
    <property name="AuthenticatorType" type="BExIS.Security.Entities.Objects.AuthenticatorType, BExIs.Security.Entities" >
      <column name="AuthenticatorType" index="idx_Authenticators_authenticatorType"/>
    </property>

    <set name="Users" inverse="true" cascade="save-update" lazy="true">
      <key column="AuthenticatorRef"/>
      <one-to-many class="BExIS.Security.Entities.Subjects.User, BExIS.Security.Entities" />
    </set>

  </class>

</hibernate-mapping>