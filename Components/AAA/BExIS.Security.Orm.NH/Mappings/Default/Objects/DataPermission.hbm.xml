<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="BExIS.Security.Entities" namespace="BExIS.Security.Entities.Objects">
  <joined-subclass xmlns="urn:nhibernate-mapping-2.2" name="DataPermission" table="DataPermissions" extends="Permission" dynamic-update="true">

    <!-- Mapping Permission Properties -->
    <key column="Id" />

    <!-- Mapping Feature Permission Associations -->
    <property name="EntityName" type="String" />

    <property name="Rights" type="byte" />

    <property name="Property" type="String" />
    <property name="Comparator" type="BExIS.Security.Entities.Objects.Comparator, BExIS.Security.Entities" />
    <property name="Value" type="String" />

  </joined-subclass>

</hibernate-mapping>